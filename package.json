{
  "name": "insightshive-website",
  "version": "1.0.0",
  "description": "AI-Powered Retail Intelligence Platform Website",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=3000",
    "dev": "live-server --port=3000 --watch=css,js,*.html",
    "lint:css": "stylelint css/**/*.css",
    "lint:js": "eslint js/**/*.js",
    "lint:html": "html-validate *.html",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:html",
    "format": "prettier --write *.html css/**/*.css js/**/*.js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "ai",
    "retail",
    "computer-vision",
    "machine-learning",
    "analytics",
    "saas"
  ],
  "author": "InsightsHive Team",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "live-server": "^1.2.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "html-validate": "^8.9.1"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
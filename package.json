{
  "name": "insightshive-website",
  "version": "1.0.0",
  "description": "AI-Powered Retail Intelligence Platform Website",
  "main": "index.html",
  "scripts": {
    "build": "npm run css:build",
    "css:build": "postcss css/main.css -o css/main.css --use postcss-import autoprefixer",
    "css:minify": "postcss css/main.css -o css/main.css --use postcss-import autoprefixer cssnano",
    "lint": "eslint js/**/*.js --no-eslintrc --config .eslintrc.js",
    "lint:fix": "eslint js/**/*.js --fix --no-eslintrc --config .eslintrc.js",
    "validate": "html-validate *.html --config .htmlvalidate.json"
  },
  "keywords": [
    "ai",
    "retail",
    "computer-vision",
    "analytics",
    "insightshive"
  ],
  "author": "InsightsHive Team",
  "license": "MIT",
  "dependencies": {
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^15.1.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.2",
    "eslint": "^8.57.0",
    "html-validate": "^8.7.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/insightshive"
  },
  "homepage": "https://YOUR_USERNAME.github.io/insightshive"
}